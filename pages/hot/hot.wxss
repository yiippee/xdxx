
/*中间描述文字样式*/
.centerContent{
  margin-top: 20rpx;
  width: 100%;
  height: 600rpx;

}

/*视频*/
.content-multimedia{
  display: inline-flex;
  margin-top: 20rpx;
  flex-direction: row;
  justify-content: center;
  max-width:100%;
  height: auto;
}

.controls {
  position: relative;
  display: flex;
}

.play, .pause {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 50px;
}